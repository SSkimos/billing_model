{
    "version": 3,
    "name": "simpleapi gw",
    "endpoints": [
        {
            "endpoint": "/server",
            "output_encoding": "no-op",
            "method": "GET",
            "backend": [
                {
                    "host": [
                        "http://server_api:5001"
                    ],
                    "encoding": "no-op",
                    "url_pattern": "/"
                }
            ]
        },
        {
            "endpoint": "/transaction",
            "output_encoding": "json",
            "method": "GET",
            "backend": [
                {
                    "host": [
                        "http://transaction_api:5002"
                    ],
                    "url_pattern": "/",
                    "encoding": "json"
                }
            ]
        }
    ]
}
